    "graphql-tag": "^2.11.0",
    "if-env": "^1.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3"
  },
  "devDependencies": {
    "concurrently": "^5.3.0"
  }
}
